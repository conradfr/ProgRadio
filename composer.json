{
    "name": "conradfr/progradio",
    "license": "NPOSL-3.0",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AppBundle\\": "tests/AppBundle",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "darsyn/ip": "^4.0",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^4.10",
        "gedmo/doctrine-extensions": "^3.0",
        "karser/karser-recaptcha3-bundle": "^0.1.22",
        "keiko/uuid-shortener": "^1.0.0",
        "knplabs/knp-paginator-bundle": "^6.3",
        "liip/imagine-bundle": "^2.7",
        "oro/doctrine-extensions": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^2.2.0",
        "qbbr/pgsql-doctrine-random-function": "^1.0",
        "snc/redis-bundle": "^4.7.1",
        "stof/doctrine-extensions-bundle": "^1.8",
        "symfony/asset": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/error-handler": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^2",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "^7.1",
        "symfony/uid": "7.1.*",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9.0",
        "phpstan/phpstan-doctrine": "^1.3.0",
        "phpstan/phpstan-symfony": "^1.2.0",
        "phpstan/phpdoc-parser": "^1.29",
        "rector/rector": "^0.15.10",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "symfony/var-dumper": "7.1.*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@deploy-cache"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "classmap": [
        "bin/.phpunit/phpunit/src"
    ]
}
