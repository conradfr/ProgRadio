{
    "name": "conradfr/progradio",
    "license": "NPOSL-3.0",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AppBundle\\": "tests/AppBundle",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "aldaflux/pgsql-doctrine-random-function": "dev-master",
        "composer/package-versions-deprecated": "1.11.99.1",
        "darsyn/ip": "^5.0",
        "darsyn/ip-doctrine": "^6.0",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "easycorp/easyadmin-bundle": "^4.15.0",
        "friendsofphp/proxy-manager-lts": "*",
        "gedmo/doctrine-extensions": "^3.0",
        "karser/karser-recaptcha3-bundle": "^0.1.22",
        "keiko/uuid-shortener": "^1.0.0",
        "knplabs/knp-paginator-bundle": "^6.3",
        "liip/imagine-bundle": "^2.7",
        "meilisearch/search-bundle": "^0.15.7",
        "oro/doctrine-extensions": "dev-master",
        "pentatrion/vite-bundle": "^8.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^3.1.0",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sentry/sentry-symfony": "^5.2",
        "snc/redis-bundle": "^4.7.1",
        "stof/doctrine-extensions-bundle": "^1.8",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/error-handler": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpdoc-parser": "^2.3.0",
        "phpstan/phpstan": "^2.1.0",
        "phpstan/phpstan-doctrine": "^2.0.0",
        "phpstan/phpstan-symfony": "^2.0.0",
        "rector/rector": "^2.2.0",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "7.3.*",
        "symfony/stopwatch": "7.3.*",
        "symfony/var-dumper": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@deploy-cache"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "classmap": [
        "bin/.phpunit/phpunit/src"
    ]
}
